<h1>Upload a New Dataset</h1>

<%= form_with model: @dataset, local: true do |form| %>
  <div class="field">
    <%= form.label :name %>
    <%= form.text_field :name %>
  </div>

  <div class="field">
    <%= form.label :file %>
    <%= form.file_field :file %>
  </div>

  <div class="actions">
    <%= form.submit "Upload Dataset" %>
  </div>
<% end %>